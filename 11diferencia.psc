Algoritmo absoluto
	Escribir "Dame el primero"
	Leer primero
	Escribir "Dame el segundo"
	Leer segundo
	diferencia=primero-segundo 
	si diferencia>0
		Escribir "valor absoluto = " diferencia	
	FinSi
	si diferencia<0
		diferencia=segundo-primero
		Escribir "valor absoluto = " diferencia
FinSi
FinAlgoritmo
